package Flowerbed;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner cs = new Scanner(System.in);
		homeWorkOne(cs);
	}
	public static void homeWorkOne(Scanner cs){
		
		int radius = cs.nextInt();
		double Pi = 3.14;
		System.out.println("S kola: [ " + Pi * radius * radius + " ]");
	}
}
