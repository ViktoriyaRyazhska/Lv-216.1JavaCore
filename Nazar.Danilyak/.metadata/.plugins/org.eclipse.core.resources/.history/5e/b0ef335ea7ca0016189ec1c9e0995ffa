package HW4.T1;

public class Month {
	private int[] month = new int[12];

	public Month() {
		//{31,28,31,30,31,30,31,31,30,31,30,31}
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
		month[0]=31;
	
	}

	public int[] getMonth() {
		return month;
	}

}
