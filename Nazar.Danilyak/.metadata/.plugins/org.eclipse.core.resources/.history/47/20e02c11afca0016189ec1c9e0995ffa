package HW4.T3;

public class Task3 {
	private int[] array;

	public Task3(int[] array) {

		this.array = array;
	}
	public int indexTwoInt(){
		int tempIndex=1;
		for (int i = 0; i < array.length; i++) {
			if(tempIndex==2){
				return array[i];
			}
			if(array[i]>=0){
				tempIndex++;
			}
			
		}
		return -1;
	}
	public int min(){
		int min=array[0];
		for (int i = 0; i < array.length; i++) {
			for (int j = 1; j < array.length; j++) {
				if(array[j]<array[i]){
					int temp=array[i];
					array[i]=array[j]
				}
			}
		}
	}
}
